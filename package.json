  "scripts": {
    "analyze:lint": "eslint --ext .tsx --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,tsx,json,md,mdx}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .tsx --ext .ts . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,ts,jsx,tsx,json}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "develop": "gatsby develop",
    "start": "run-p start:**",
    "start:app": "npm run develop",
    "start:lambda": "netlify-lambda serve /functions",
    "build": "gatsby build",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build /functions",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "./node_modules/typescript/bin/tsc --noEmit --skipLibCheck",
    "token": "gatsby-source-google-photos-token",
    "lint": "eslint . --ignore-path .gitignore --ext .md,.mdx,.ts,.tsx,.js",
    "deploy": "gatsby build && gh-pages -d public -b main"
