    "analyze:lint": "eslint --ext .tsx --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,tsx,json,md,mdx}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .tsx --ext .ts . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,ts,jsx,tsx,json}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
